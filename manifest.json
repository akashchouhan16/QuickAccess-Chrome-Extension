{
    "manifest_version" : 2,
    "name" : "akashchouhan16 links",
    "version" : "1.1.0",
    "description" : "A basic Chrome extension that will keep all my links at one place, and to block out all the Unschool bs on the web. :)",
    "icons": { 
          "96": "icon.png" },
    "browser_action": {
              "default_icon" : "icon.png",
              "default_popup" : "popup.html"
          },
    "permissions": ["activeTab"],
    "content_scripts" : [
        {
            "matches": ["<all_urls>"],
            "js" : ["popup.js"]
        }
    ]
}